{
  "name": "swiftsale",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "dev:env": "dotenv -e ./config/dev.env -- next dev",
    "dev:env:example": "dotenv -e ./config/dev.env.example -- next dev",
    "prisma": "dotenv -e ./config/dev.env -- prisma",
    "prisma:env:example": "dotenv -e ./config/dev.env.example -- prisma",
    "db:push": "dotenv -e ./config/dev.env -- prisma db push",
    "db:push:env:example": "dotenv -e ./config/dev.env.example -- prisma db push",
    "db:migrate": "dotenv -e ./config/dev.env -- prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "next": "14.2.24",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.24",
    "prisma": "^5.8.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}